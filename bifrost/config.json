{
  "client": { "drop_excess_requests": false },
  "providers": {
    "openai": {
      "keys": [ { "value": "env.OPENAI_API_KEY", "models": ["gpt-4o-mini", "gpt-4o"] } ]
    },
    "google": {
      "keys": [ { "value": "env.GOOGLE_API_KEY", "models": ["gemini-1.5-pro-latest","gemini-1.5-flash-latest"] } ]
    }
  },
  "config_store": {
    "enabled": true,
    "type": "sqlite",
    "config": { "path": "./data/config.db" }
  }
}
